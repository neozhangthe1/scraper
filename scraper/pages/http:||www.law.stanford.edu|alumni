  $(document).ready(function() {
    $("#open").click(function () {
      $("#megamenu").slideDown("slow");
      $("#open").hide();
      $("#sls_online_bkgr").hide();
    });
    $("#close").click(function () {
      $("#megamenu").slideUp("slow");
      $("#sls_online_bkgr").show();
      $("#open").show();
    });
  });
</script></div> <!-- /.node -->
