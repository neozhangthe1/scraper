function recordOutboundLink(link, category, action) {
try {
var myTracker=_gat._getTrackerByName();
_gaq.push(['myTracker._trackEvent', category , action ]);
setTimeout('document.location = "' + link.href + '"', 100)
}catch(err){}
}

 // adding events tracking to any link https://developers.google.com/analytics/devguides/collection/gajs/eventTrackerGuide   _trackEvent(category, action, opt_label, opt_value, opt_noninteraction)
 	  
function wrapgaq1(sel, a){
	jQuery(sel).click(function() {
	  var txt = this.innerHTML;
 	  txt = txt.replace(/&amp;/g,"and");
	  _gaq.push(['_trackEvent', a, txt  ]);
 	});
}

 
function wrapgaq3(sel, a, b){
 		jQuery(sel).click(function() {
		var txt = this.innerHTML;
		txt = txt.replace(/&amp;/g,"and");
		_gaq.push(['_trackEvent', a,  b, txt ]);
	});
}



jQuery(document).ready(function() {
								
	// main navigation
    wrapgaq3("#school a", 'Main Navigation', 'The School');  //
    wrapgaq3("#program a", 'Main Navigation', 'The Program');  //
    wrapgaq3("#experience a", 'Main Navigation', 'The Experience');  //
								
    wrapgaq1("#breadcrumbs > div > a", 'Breadcrumb' );
  	
    wrapgaq1(".region-sidebar-first a", 'Left Sidebar');   
    wrapgaq1(".region-sidebar-second a", 'Right Sidebar');   
 
	// Across the Social Mediascape for Areas of interest and Organizations
    wrapgaq1("#feeds a", 'Across the Social Mediascape' );
 	
    wrapgaq1("#footer div  a", 'Footer' );
   	
 });

</script>

